{
  "name": "pnc-ui",
  "version": "2.0.0",
  "engines": {
    "node": "^10.8.0",
    "npm": "^6.2.0"
  },
  "scripts": {
    "test": "exit 0",
    "build": "node ./scripts/generate-index && npm run compile:html && npm run compile:js && npm run compile:css && npm run copy",
    "prebuild": "npm run clean && mkdir dist && npm install && npm test",
    "start": "budo index.js --dir .tmp/ --live -- -t [ babelify ] -p [ parcelify -o .tmp/lib.css ]",
    "prestart": "rm -rf .tmp/* && cp src/index.html .tmp && cp -R assets/images .tmp/",
    "clean": "rm -rf dist; rm -rf ./tmp",
    "clean:force": "npm run clean && rm -rf node_modules",
    "compile:js": "babel ./src/pnc{,/**}/*.module.js src/pnc{,/**}/!(*.module).js src/index.js --out-file .tmp/index.js --source-maps",
    "compile:css": "cat assets/styles/*.css > dist/index.css",
    "compile:html": "nghtml2js -m 'pnc.templates' -s 'browser' -f 'src/pnc/**/*.html' -p 'src/' -o '.tmp/templateCache.js'",
    "deps:js": "babel $(node ./scripts/bundle.js) --out-file .tmp/lib.js --source-maps --compact",
    "copy": "cp src/index.html dist/ && cp -R assets/images dist/"
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.3",
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-resource": "1.5.8",
    "angular-touch": "1.5.8",
    "angular-loading-bar": "^0.9.0",
    "angular-patternfly": "^4.18.1",
    "angular-ui-bootstrap": "2.2.0",
    "angular-utils-pagination": "^0.11.1",
    "angular-websocket": "2.0.0",
    "angular-xeditable": "^0.1.8",
    "jquery": "^2.1.4",
    "lodash": "^4.0.0",
    "ng-infinite-scroll": "^1.3.0",
    "patternfly": "^3.54.1",
    "patternfly-bootstrap-treeview": "^2.1.0"
  },
  "devDependencies": {
    "angular-template-cache": "^1.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "budo": "^11.3.2",
    "glob": "^7.1.2",
    "parcelify": "^2.2.0"
  }
}
